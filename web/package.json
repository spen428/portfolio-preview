{
  "name": "web",
  "version": "2.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "prod": "NODE_ENV=production vite --host",
    "build:prod": "NODE_ENV=production vue-tsc --noEmit && vite build",
    "lint": "prettier --check . && eslint . --ext .vue,.js,.ts",
    "lint:fix": "prettier --write . && eslint . --ext .vue,.js,.ts --fix",
    "test": "true",
    "test:local": "rm -rf visual_regressions/bitmaps_test && PROJECT_NAME=portfolio-test-local IMAGE_TAG=local bash ../.teamcity/scripts/test-with-real-data.sh && xdg-open web/visual_regressions/html_report/index.html",
    "test:without-docker": "backstop test --configPath=backstop.cjs",
    "approve": "backstop approve --configPath=backstop.cjs"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "1.5.0",
    "axios": "1.6.2",
    "luxon": "3.4.4",
    "markdown-it": "14.0.0",
    "shared": "workspace:shared",
    "vue": "3.3.4",
    "vue-i18n": "9.8.0",
    "vue-router": "4.2.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.3.0",
    "@types/luxon": "3.4.2",
    "@types/markdown-it": "13.0.7",
    "@vitejs/plugin-vue": "4.2.3",
    "@vitejs/plugin-vue-jsx": "3.0.1",
    "@vue/eslint-config-prettier": "7.1.0",
    "@vue/eslint-config-typescript": "11.0.3",
    "@vue/tsconfig": "0.4.0",
    "autoprefixer": "10.4.14",
    "eslint-plugin-vue": "9.14.1",
    "postcss": "8.4.24",
    "postcss-import": "15.1.0",
    "prettier-plugin-tailwindcss": "0.3.0",
    "puppeteer": "21.6.1",
    "tailwindcss": "3.3.2",
    "vite": "4.3.9",
    "vue-tsc": "1.6.5"
  }
}
